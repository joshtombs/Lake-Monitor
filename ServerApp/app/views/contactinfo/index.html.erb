<p id="notice"><%= notice %></p>

<h1>All Contacts:</h1>

<table class="table">
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Phone Number</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.first_name %></td>
        <td><%= contact.last_name %></td>
        <td><%= contact.phone_number %></td>
        <td><%= contact.email %></td>
        <!-- Link to show -->
        <!-- Link to destroy -->
      </tr>
    <% end %>
  </tbody>
</table>