<p id="notice"><%= notice %></p>

<p>
  <table class="table">
  <thead>
    <tr>
      <th>Time Recorded</th>
      <th>SensorID</th>
      <th>Type</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @daily_readings.each do |data| %>
      <tr>
        <td><%= data.time_recorded %></td>
        <td><%= data.sensor_id %></td>
        <td><%= data.type.titleize %></td>
        <td><%= data.value %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</p>

<p>
  <a href="<%=@previous_link%>">Back</a>
</p>
